{
    "manifest_version": 3,
    "name": "Busca CNS/CPF",
    "version": "1.0",
    "description": "Busca CNS por CPF e CPF por CNS no SISREG",
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://regulador.saude.pe.gov.br/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "permissions": [
      "activeTab",
      "scripting",
      "tabs"
    ],
    "host_permissions": [
      "https://sisregiii.saude.gov.br/*",
      "https://regulador.saude.pe.gov.br/*"
    ],
    "icons": {
      "16": "icone.png",
      "32": "icone.png",
      "48": "icone.png",
      "128": "icone.png"
    }
  }
  